
<div class="row">
    <div class="col-9">
        <h2 class="text-primary">Flight Categories</h2>
    </div>
    <div class="col-3">
        <a asp-action="SaveOrEdit" class="btn btn-info form-control">
            <i class="fas fa-plus">&nbsp;Add Category</i>
        </a>
    </div>
</div>
<div class="container rounded border p-2 m-2">
    <table id="tblData" class=" table table-bordered table-striped table-active">
        <thead>
            <tr>
                
                <th>Name</th>
                <th>Price</th>
                <th>Actions</th>
            </tr>
        </thead>
    </table>
</div>
@section scripts
{
    <script src="~/js/flightcategory.js"></script>
    <partial name="_ValidationScriptsPartial" />
   
}
